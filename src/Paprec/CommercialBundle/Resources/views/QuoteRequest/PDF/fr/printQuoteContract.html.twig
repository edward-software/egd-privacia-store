<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//FR"
        "http://www.w3.org/TR/html4/strict.dtd">
<html>
<head>
    <title>PDF EDW</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <style type="text/css">
        html,
        body {
            margin: 0;
            padding: 0;
            font-family: "avenir", Arial, sans-serif;
            font-weight: 500;
            font-size: 12px;
        }

        * {
            box-sizing: border-box;
            -webkit-box-sizing: border-box;
        }

        h1 {
            color: #072244;
            font-size: 16px;
            text-transform: uppercase;
            font-weight: 900;
            margin: 0;
        }

        h2 {
            color: #072244;
            font-size: 22px;
            font-weight: 700;
        }
        h3 {
            color: #072244;
            font-size: 16px;
            font-weight: 700;
        }

        p {
            margin: 10px 0;
        }

        .edw-container__body {
            /*overflow: hidden;*/
            width: 1300px;
            /*height: 895px;*/
            margin: auto;
            position: relative;
            margin-bottom: 50px;
        }

        /* FONTS */

        @font-face {
            font-family: "avenir";
            src: url("https://staging.static.edward.ws/common/fonts/AvenirLTStd-Light.otf") format("truetype");
            font-style: normal;
            font-weight: 300;
        }

        @font-face {
            font-family: "avenir";
            src: url("https://staging.static.edward.ws/common/fonts/AvenirLTStd-Medium.otf") format("truetype");
            font-style: normal;
            font-weight: 500;
        }

        @font-face {
            font-family: "avenir";
            src: url("https://staging.static.edward.ws/common/fonts/AvenirLTStd-Heavy.otf") format("truetype");
            font-style: normal;
            font-weight: 700;
        }

        @font-face {
            font-family: "avenir";
            src: url("https://staging.static.edward.ws/common/fonts/AvenirLTStd-Black.otf") format("truetype");
            font-style: normal;
            font-weight: 900;
        }

        /* Buttons */

        .edw-button--secondary {
            background-color: #DDE6FF !important;
            color: #072244 !important;
        }
        .edw-button {
            height: 24px;
            color: #072244;
            border-radius: 3px;
            border: 2px solid #072244;
            background-color: #fff;
            text-align: center;
            font-weight: 500;
            padding: 2px 5px 0 !important;
            outline: none !important;
            font-size: 12px;
            overflow: hidden;
        }

        /* Margin / Padding */

        .mb-20 {
            margin-bottom: 20px;
        }

        .mb-25 {
            margin-bottom: 25px;
        }
        .mr-15 {
            margin-right: 15px;
        }
        .text-center {
            text-align: center;
        }

        /* Icons */

        .edw-icon-ppm {
            content: url("https://staging.static.edward.ws/ppm/img/export_pdf/ppm.png");
            height: 20px;
            margin-right: 10px;
        }

        .edw-icon-arrow {
            height: 12px;
            margin: 0 5px;
        }
        .edw-icon-arrow-blue {
            content: url("https://staging.static.edward.ws/ppm/img/export_pdf/arrow_bleu.png");
        }
        .edw-icon-arrow-gray {
            content: url("https://staging.static.edward.ws/ppm/img/export_pdf/arrow_gris.png");
        }
        .edw-icon-warning {
            content: url("https://staging.static.edward.ws/ppm/img/export_pdf/warning.png");
            width: 13px;
        }


        /* FLEXBOX */

        .flex {
            display: -webkit-box;
            display: -webkit-flex;
            display: -ms-flexbox;
            display: -moz-box;
            display: box;
            display: flex;
        }

        .align-item-center {
            -webkit-box-align: center;
            -moz-box-align: center;
            -ms-flex-align: center;
            -webkit-align-items: center;
            box-align : center;
            align-items: center;
        }

        .justify-center {
            -webkit-box-pack : center;
            box-pack : center;
            justify-content: center;
        }
        .space-between {
            -webkit-box-pack : justify;
            box-pack : justify;
            justify-content: space-between;
        }

    </style>

</head>
<body>

