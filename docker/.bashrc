alias ll="/bin/ls -lah"
alias c2="php bin/console"
alias log2="tail -f var/log/*.log"
alias doclog2="tail -f var/log/dev_doctrine.log"
alias cc2="c2 cache:clear --env=dev --no-warmup"
alias dump2="c2 doctrine:schema:update --dump-sql"
alias force2="c2 doctrine:schema:update --force"
alias yarndev="yarn run encore dev --watch"
alias yarnprod="yarn run encore prod"
